curl -X 'POST' \
    'https://127.0.0.1:8000/users/' \
    -H 'accept: application/json' \
    -H 'Content-Type: application/json' \
    -d '{
    "name": "Alex",
    "email": "my@email.ru"
}'

curl -X 'POST' 'https://127.0.0.1:8000/users/' -H 'accept: application/json' -H 'Content-Type: application/json' -d '{"name": "Alex", "email": "my@email.ru"}'
